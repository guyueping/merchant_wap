.head_box
  color #ffffff
  font-size 30px
  text-align center
  width 100%
  position fixed
  top 0 
  z-index 999
.head_container
  width 100%
  height 100%
  position relative
  box-sizing border-box
  padding-top 10px
  .back_button
    position absolute
    left 0
    box-sizing border-box
    padding-left 30px
    width 140px
    height 100%
    &:before
      content ''
      width 20px
      height 40px
      // border-left 4px solid #ffffff
      // border-top 4px solid #ffffff
      display inline-block
      background-image url('data:image/png;base64,')
      background-repeat no-repeat
      background-position center center
      background-size 100%
      margin-right 10px
      // transform rotate(-45deg)
.indexPage_box 
  width 100%
  height 100%
  overflow hidden
  .top_box
    width 110%
    margin-left -5%
    height 215px 
    background linear-gradient(#4F5AF7 0%, #553BE2 100%)
    border-bottom-left-radius 50%
    border-bottom-right-radius 50%
  .amount_box
    // height 287px
    background #FFFFFF
    border-radius 12px
    margin -180px 30px 30px 30px
    box-sizing border-box
    padding 40px 20px 30px 20px
    .withdraw_box
      margin-left 30px
    .warn_text_box
      width 100%
      height 70px
      line-height 70px
      background-color #FFF9E7
      box-sizing border-box
      padding 0 30px
      font-size 24px
      color #BF7400
      margin-top 18px
      .red
        color #FF4400
      .icon_box
        width 30px
        height 30px
        margin-right 10px
        background url('data:image/png;base64,') 0 0/100% 100% no-repeat
        position relative
      .toolTip
        transform scale(0)
        transform-origin 0 0
        translation transform 0.3s
        width 500px
        background #FFFFFF
        border-radius 8px
        box-shadow 0px 2px 10px 0px rgba(0, 0, 0, 0.1)
        padding 20px
        font-size 24px
        line-height 33px
        color rgba(0, 0, 0, 0.85)
        position absolute 
        top 50px
        left -40px
        &:before
          position absolute 
          content ''
          display block
          width 0
          height 0
          border 10px solid transparent 
          border-bottom 14px solid #FFFFFF
          top -22px
          left 44px
      .show_toolTip
        transform scale(1)
    .withdraw_amount
      height 58px
      font-size 50px
      font-family DINAlternate-Bold, DINAlternate
      font-weight bold
      color #FF4400
      line-height 58px
    .withdraw_button
      width 134px
      height 50px
      line-height 50px
      color #FFFFFF
      background #4F5AF7
      border-radius 25px
      margin-left 20px
      font-size 26px
      text-align center
      &:after
        content ''
        display inline-block
        width 10px
        height 10px
        border-top 1px solid #FFFFFF
        border-right 1px solid #FFFFFF
        transform rotate(45deg)
        margin-left 4px
    .widthdraw_text
      height 33px
      font-size 24px
      color #969696
      line-height 33px
      margin-top 10px
    .amount_container
      width 100%
      display flex
      margin-top 30px
      .amount_wrap
        width 50%
        font-size 40px
        color #474245
        line-height 47px
        box-sizing border-box
        padding-left 30px
        .amount_name
          margin-top 6px
          font-size 24px
          color #969696
          line-height 33px
        &:not(:first-child)
          position relative 
          &:before
            content ''
            display block
            width 1px 
            height 72px
            position absolute
            left -1px
            top 6px
            background-color #D8D8D8

  .map_box
    height 502px
    background #FFFFFF
    border-radius 12px
    margin 30px 30px 40px 30px
    box-sizing border-box
    padding 20px 30px
    .title
      height 42px
      font-size 30px
      font-family PingFangSC-Semibold, PingFang SC
      font-weight 600
      color #474245
      line-height 42px
      padding-bottom 20px
      border-bottom 1px solid #F1F1F1
    .grid_box
      height 37px
      font-size 26px
      color #474245
      line-height 37px
      padding-top 10px
      .menu_box
        width 156px
        height 196px
        .icon_box
          width 80px
          height 80px
          border-radius 40px
          overflow hidden
          background-image url('../../assets/images/icon_todo.png')
          background-size 100% 100%
          background-position 0 0
          background-repeat no-repeat
          margin-bottom 23px
